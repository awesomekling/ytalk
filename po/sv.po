#: src/main.c:206
#, c-format
msgid ""
"Usage:    %s [options] user[@host][#tty]...\n"
"Options:     -i             --    no auto-invite port\n"
"             -Y             --    require caps on all y/n answers\n"
"             -s             --    start a shell\n"
"             -q             --    prompt before quitting\n"
"             -v             --    print program version\n"
"             -h             --    show this help message\n"
msgstr ""
"Användning: %s [flaggor] användare[@dator][#tty]...\n"
"Flaggor:        -i          --    stäng av auto-inbjudan\n"
"                -Y          --    kräv versaler i j/n-svar\n"
"                -s          --    börja i ett skal\n"
"                -q          --    fråga innan vi avslutar\n"
"                -v          --    skriv ut programversion\n"
"                -h          --    visa detta hjälpmeddelande\n"

